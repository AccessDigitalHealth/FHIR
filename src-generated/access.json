      "template-profile-ttl": "../framework/templates/template-profile-ttl.html",
      "base": "{{[id]}}.html",
      "defns": "{{[id]}}-definitions.html",
      "mappings": "{{[id]}}-mappings.html",
      "examples": "{{[id]}}-examples.html",
